#include "Vec3.h"


 Vec3::Vec3(int x, int y, int z):_x(x),_y(y),_z(z){}
 
 Vec3::~Vec3(){}
 
 int Vec3::getX(){
 	return _x;
 }
 
  int Vec3::getY(){
 	return _y;
 }
 
  int Vec3::getZ(){
 	return _z;
 }
 
 void Vec3::operator+ (const Vec3 &op){
 	
 	int n_x, n_y, n_z;
 	
 	n_x = op.getX() - this->_x;
 	n_y = op.getY() - this->_y;
 	n_z = op.getZ() - this->_z;
 	
 	//Sin return de objeto dado el "esqueleto"
 }
 
  void Vec3::operator- (const Vec3 &op){
 	
 	int n_x, n_y, n_z;
 	
 	n_x = op.getX() - this->_x;
 	n_y = op.getY() - this->_y;
 	n_z = op.getZ() - this->_z;
 }
